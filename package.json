{
  "name": "who-to-support",
  "description": "find the right projects to sponsor",
  "version": "0.0.0",
  "author": "Marco Link @marcolink",
  "bin": {
    "who-to-support": "./bin/run"
  },
  "types": "lib/index.d.ts",
  "bugs": "https://github.com/marcolink/who-to-support/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@supercharge/promise-pool": "^1.6.0",
    "chalk": "^4.1.0",
    "cli-table": "^0.3.4",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "listr": "^0.14.3",
    "node-emoji": "^1.10.0",
    "node-fetch": "^2.6.1",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@semantic-release/changelog": "^5.0.1",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/cli-table": "^0.3.0",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^8",
    "@types/node": "^14",
    "@types/node-fetch": "^2.5.7",
    "chai": "^4",
    "eslint": "^8.16",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2",
    "mocha": "^5",
    "nyc": "^15",
    "semantic-release": "^17.3.0",
    "ts-node": "^9",
    "type-fest": "^0.20.2",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/marcolink/who-to-support",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "who-to-support"
  },
  "repository": "marcolink/who-to-support",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "build": "tsc -b",
    "presemantic-release": "yarn run build",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/github"
    ]
  }
}
